<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WatFit"
        tools:targetApi="31">
        <activity
            android:name=".SharedWithMeActivity"
            android:exported="false"
            android:label="@string/title_shared_with_me"/>
        <activity
            android:name=".FitnessGoalActivity"
            android:exported="false"
            android:label="@string/title_fitness_goal"/>
        <activity
            android:name=".PasswordResetActivity"
            android:exported="false"
            android:label="@string/title_password_reset" />
        <activity
            android:name=".AccountActivity"
            android:exported="false"
            android:label="@string/title_account" />
        <activity
            android:name=".BasicDiameterActivity"
            android:exported="false"
            android:label="@string/title_basic_diameter" />
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:label="@string/title_login" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false"
            android:label="@string/title_register" />
        <activity
            android:name=".StartActivity"
            android:exported="true"
            android:label="@string/title_start" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>